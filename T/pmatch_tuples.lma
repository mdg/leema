import io

func match_zero x:Int y:Int /Str
>>
    match (x, y)
    |(0, 0) >> "both"
    |(0, _) >> "first"
    |(_, 0) >> "second"
    |_ >> "neither"
    --
--

func main >>
    io::print(match_zero(0, 0) \n)
    io::print(match_zero(0, 1) \n)
    io::print(match_zero(2, 0) \n)
    io::print(match_zero(4, 3) \n)
--
