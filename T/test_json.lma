
import json
import map


func test_decode() >>
    let i := json::decode("\"coded  '\"")
    print(i \n)
--

func test_encode() >>
    print(json::encode(6) \n)
    print(json::encode(false) \n)
    print(json::encode("hello") \n)
    print(json::encode(#world) \n)
    print(json::encode(["a", "b"]) \n)
    let m := map::set({}, "x", 4)
    print(json::encode(m) \n)
--

func main() >>
    test_decode()
    test_encode()
--
