
import /io.print


const HEX_RED :=   "#ff0000"
const HEX_GREEN := "#00ff00"
const HEX_BLUE :=  "#0000ff"

datatype Rgb ::
    red: Int
    green: Int
    blue: Int
--

func main ->
    let c := Rgb(10, 20, 30)
    print("color: $c\n")
    print("red: ${c.red}\n")
    print("blue: ${c.blue}\n")
    let hex_g := HEX_GREEN
    print("hex green is: $hex_g\n")
--
