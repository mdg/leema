import /io

func foo a:[#]
|[] -> io.print("done\n")
|#c;t ->
    io::print("found c.\n")
    foo(t)
|h;t ->
    match h
    |#a -> io.print("found a! what else is in $t ?\n")
    |#b -> io.print("found b! what else is in $t ?\n")
    |#c -> io.print("found c! what else is in $t ?\n")
    --
    foo(t)
--

func main -> foo([#a, #b, #c]) --
