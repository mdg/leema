
func foo(a: [#]) >>
|([]) -> print("done\n")
|(#c;t) ->
    print("found c.\n")
    foo(t)
|(h;t) ->
    match h
    |#a -> print("found a! what else is in $t ?\n")
    |#b -> print("found b! what else is in $t ?\n")
    |#c -> print("found c! what else is in $t ?\n")
    --
    foo(t)
--

func main() -> foo([#a, #b, #c]) --
