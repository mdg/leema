import io

func fizzbuzz x:Int max:Int :Void
>>
    let xm30 := (x mod 3) == 0
    let xm50 := (x mod 5) == 0

    if
    |xm30 and xm50 >> io::print("fizzbuzz\n")
    |xm30 >> io::print("fizz\n")
    |xm50 >> io::print("buzz\n")
    |else >> io::print("$x\n")
    --

    if x < max >>
        fizzbuzz(x+1, max)
    --
--


func main >> fizzbuzz(1, 100) --
