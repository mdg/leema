func fizzbuzz(x, max): Void >>
    let xm30 := (x mod 3) == 0
    let xm50 := (x mod 5) == 0

    if
    |xm30 and xm50 >> print("fizzbuzz\n")
    |xm30 >> print("fizz\n")
    |xm50 >> print("buzz\n")
    |else >> print("$x\n")
    --

    if x < max >>
        fizzbuzz(x+1, max)
    --
--


func main() >> fizzbuzz(1, 100) --
